{
  "name": "flowshare-ui",
  "version": "1.0.0",
  "description": "The web user interface of the flowshare project",
  "main": "index.js",
  "scripts": {
    "build": "yarn build:typescript && npm run build:babel && yarn build:scss",
    "build:javascript": "yarn build:typescript && yarn build:babel",
    "build:typescript": "tsc --emitDeclarationOnly",
    "build:babel": "babel src/typescript --out-dir dist/javascript --extensions \".ts,.tsx\" --source-maps true",
    "build:scss": "sass src/scss:dist/css --style=compressed && postcss dist/css/*.css --replace --no-map --use autoprefixer postcss-preset-env postcss-flexbugs-fixes"
  },
  "author": "cefadrom",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "autoprefixer": "^9.7.3",
    "babel-plugin-minify-mangle-names": "^0.5.0",
    "babel-preset-minify": "^0.5.1",
    "postcss": "^7.0.24",
    "postcss-cli": "^6.1.3",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-preset-env": "^6.7.0"
  },
  "dependencies": {
    "@types/jquery": "^3.3.31",
    "@types/js-cookie": "^2.2.4",
    "js-cookie": "^2.2.1"
  },
  "repository": "https://github.com/cefadrom/flowshare-ui",
  "private": true
}
